#include"main.h"
int main(void)
{
	int i = 0;

	while (i < 1024)
	{
		int sum;

		if (i % 3 == 0 || i % 5 == 0)
		{
			sum += i;
			if (sum <= 9)
			{
				ones(sum);
			}
			else if (sum > 9 && sum < 100)
			{
				tens(sum);
			}
			else if (sum >= 100)
			{
				hundreds(sum);
			}
		}
		_putchar(10);
		i++;
	}
	return (0);
}
